{
  "rewrites": [
    { "source": "/docs", "destination": "/docs/index.html" },
    { "source": "/docs/(.*)", "destination": "/docs/index.html" }
  ],
  "headers": [
    {
      "source": "/docs/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ],
  "cleanUrls": true
}
